+        Snackbar.make(view, msg,Snackbar.LENGTH_LONG).setAction("Ok", new View.OnClickListener() {
+            public void onClick(View v) {
+                if (runnable != null) {
+                    runnable.run();
+                }
+
+            }
+        }).show();
+    }
